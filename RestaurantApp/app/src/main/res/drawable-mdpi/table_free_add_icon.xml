<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="40dp"
        android:height="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="2dp"
                android:color="@color/colorTableGroupAddIconStrokeColor"
                android:dashGap="4dp"
                android:dashWidth="7dp" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:width="3dp"
        android:height="20dp"
        android:gravity="right|center"
        android:right="18dp"
        android:top="-3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorTableGroupAddIconColor" />
        </shape>
    </item>
    <item
        android:width="20dp"
        android:height="3dp"
        android:gravity="right|center"
        android:right="9.5dp"
        android:top="-4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorTableGroupAddIconColor" />
        </shape>
    </item>
</layer-list>
